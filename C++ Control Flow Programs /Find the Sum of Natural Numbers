// C++ Program to Find the Sum of Natural Numbers.

#include<iostream>


using namespace std;
int main(){
    int n;
    cout<<"Enter the number ";
    cin>>n;
    int sum=0;
    for(int i=1;i<=n;i++){
            sum=sum+i;
            
    }
        cout<<"The sum of n Natural Number is "<<sum<<endl;
}
